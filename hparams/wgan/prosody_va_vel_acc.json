{
    "Dir": {
        "processed": "./data/takekuchi/processed/prosody",
        "log": "results"
    },

    "Data": {
        "valid_ratio": 0.1,
        "chunklen": 34,
        "seedlen": 4,
        "extraversion": {
            "high": 0,
            "mid": 1,
            "low": 2
        }
    },

    "Infer": {
        "pre_trained": "",
        "extraversion": ""
    },

    "Model": {
        "Generator": {
            "name": "PoseGenerator",
            "n_poses": 34,
            "noise_dim": 20,
            "hidden": 256,
            "num_layers": 2,
            "layernorm": true,
            "dropout": 0.2,
            "activation": "leaky_relu"
        },
        "Discriminator": {
            "name": "ConvDiscriminator",
            "n_poses": 34,
            "hidden_size": 128,
            "batchnorm": false,
            "layernorm": true,
            "activation": "leaky_relu"
        }
    },

    "Train": {
        "n_epochs": 150,
        "batch_size": 128,
        "lr": 1e-4,
        "n_critic": 10,
        "gp_lambda": 10,
        "gp_zero_center": false,
        "cl_lambda": 1,
        "log_gap": 1500,
        "use_vel": true,
        "use_acc": true
    },

    "device": "cuda",
    "seed": 0,
    "run_name": "prosody_va_vel_acc"
}