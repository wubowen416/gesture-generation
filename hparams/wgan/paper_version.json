{
    "Dir": {
        "processed": "data/takekuchi/processed/prosody_flt",
        "log": "results"
    },

    "Data": {
        "valid_ratio": 0.1,
        "chunklen": 34,
        "seedlen": 4
    },

    "Infer": {
        "pre_trained": ""
    },

    "Model": {
        "Generator": {
            "noise_dim": 20,
            "hidden": 256,
            "num_layers": 2,
            "layernorm": true,
            "dropout": 0.2
        },
        "Discriminator": {
            "hidden": 128,
            "batchnorm": false,
            "layernorm": true,
            "dropout": 0
        }
    },

    "Train": {
        "n_epochs": 150,
        "batch_size": 128,
        "lr": 1e-4,
        "n_critic": 10,
        "gp_lambda": 10,
        "gp_zero_center": false,
        "cl_lambda": 1,
        "log_gap": 1500,
        "use_vel": false
    },

    "device": "cuda",
    "seed": 0
}