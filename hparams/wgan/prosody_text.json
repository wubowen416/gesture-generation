{
    "Dir": {
        "processed": "data/takekuchi/processed/prosody_text",
        "log": "results"
    },

    "Data": {
        "valid_ratio": 0.1,
        "chunklen": 34,
        "seedlen": 4,
        "num_sample_per_generator": 100
    },

    "Infer": {
        "pre_trained": ""
    },

    "Model": {
        "Generator": {
            "name": "PoseGenerator",
            "noise_dim": 20,
            "hidden_size": 256,
            "num_layers": 2,
            "dropout": 0.2,
            "layernorm": true,
            "batchnorm": false,
            "n_poses": 34,
            "activation": "leaky_relu"
        },
        "Discriminator": {
            "name": "ConvDiscriminator",
            "hidden_size": 128,
            "batchnorm": false,
            "layernorm": true,
            "n_poses": 34,
            "activation": "leaky_relu"
        }
    },

    "Train": {
        "n_epochs": 150,
        "batch_size": 128,
        "n_critic": 10,
        "lr": 1e-4,
        "gp_lambda": 10,
        "gp_zero_center": false,
        "cl_lambda": 1,
        "log_gap": 1500,
        "grad_norm_value": 20,
        "grad_clip_value": 5,
        "use_vel": true,
        "use_acc": true
    },

    "device": "cuda",
    "seed": 1,
    "run_name": "gan_prosody_text"
}