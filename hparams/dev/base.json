{
    "Dir": {
        "processed": "data/takekuchi/processed/prosody",
        "log": "results"
    },

    "Data": {
        "valid_ratio": 0.1,
        "chunklen": 34,
        "seedlen": 4
    },

    "Infer": {
        "pre_trained": ""
    },

    "Model": {
        "Generator": {
            "d_noise": 20,
            "d_model": 256,
            "num_layers": 2,
            "dropout": 0.1
        },
        "CondDiscriminator": {
            "d_model": 128
        },
        "PoseDiscriminator": {
            "d_model": 128
        }
    },

    "Train": {
        "n_epochs": 150,
        "batch_size": 128,
        "n_critic": 10,
        "lr": 1e-4,
        "gp_lambda": 10,
        "cl_lambda": 1,
        "log_gap": 1500
    },

    "device": "cuda",
    "seed": 1,
    "run_name": "prosody_disc_pose"
}